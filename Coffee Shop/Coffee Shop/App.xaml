<Application x:Class="Coffee_Shop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Coffee_Shop"
             Startup="OnStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/StaticFiles/Resources/Lang.en-US.xaml" />
                <ResourceDictionary Source="/StaticFiles/Themes/Dark.xaml" />
                <ResourceDictionary Source="/StaticFiles/Styles/CommonStyles.xaml" />
                
                
                
                
                <ResourceDictionary Source="/StaticFiles/Styles/MainStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
